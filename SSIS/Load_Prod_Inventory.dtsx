      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3E793571-9E68-420F-BF1C-F4ED34B438B3}"
          SQLTask:SqlStatementSource="MERGE INTO PROD_DAILY_SUMMARY p&#xA;USING (&#xA;  SELECT product_id,&#xA;         location_id,&#xA;         TRUNC(trans_date) AS trans_date,&#xA;         SUM(CASE WHEN transaction_type='RECEIPT' THEN quantity ELSE 0 END) AS produced_qty,&#xA;         SUM(CASE WHEN transaction_type='ISSUE' THEN quantity ELSE 0 END) AS issued_qty,&#xA;         COUNT(*) AS txn_count&#xA;  FROM PROD_STG_INV_TRX&#xA;  GROUP BY product_id, location_id, TRUNC(trans_date)&#xA;) s&#xA;ON (p.product_id = s.product_id &#xA;    AND p.location_id = s.location_id &#xA;    AND p.trans_date = s.trans_date)&#xA;WHEN MATCHED THEN UPDATE &#xA;   SET p.produced_qty = s.produced_qty,&#xA;       p.issued_qty   = s.issued_qty,&#xA;       p.txn_count    = s.txn_count,&#xA;       p.last_refresh_ts = SYSDATE&#xA;WHEN NOT MATCHED THEN INSERT &#xA;   (product_id, location_id, trans_date, produced_qty, issued_qty, txn_count)&#xA;   VALUES &#xA;   (s.product_id, s.location_id, s.trans_date, s.produced_qty, s.issued_qty, s.txn_count)&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
